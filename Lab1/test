#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xbfc130 .scope module, "mux2to1_df" "mux2to1_df" 2 25;
 .timescale 0 0;
v0xbd8ee0_0 .net "a", 0 0, C4<z>; 0 drivers
v0xc0f850_0 .net "b", 0 0, C4<z>; 0 drivers
v0xc0f8f0_0 .net "f", 0 0, L_0xc10600; 1 drivers
v0xc0f990_0 .net "s", 0 0, C4<z>; 0 drivers
L_0xc10600 .functor MUXZ 1, C4<z>, C4<z>, C4<z>, C4<>;
S_0xbfc220 .scope module, "mux2to1_gl" "mux2to1_gl" 2 1;
 .timescale 0 0;
L_0xc10760 .functor NOT 1, C4<z>, C4<0>, C4<0>, C4<0>;
L_0xc10820 .functor AND 1, C4<z>, C4<z>, C4<1>, C4<1>;
L_0xc10930 .functor AND 1, C4<z>, L_0xc10760, C4<1>, C4<1>;
L_0xc10a10 .functor OR 1, L_0xc10820, L_0xc10930, C4<0>, C4<0>;
v0xc0fa40_0 .net "a", 0 0, C4<z>; 0 drivers
v0xc0fb00_0 .net "b", 0 0, C4<z>; 0 drivers
v0xc0fba0_0 .net "c", 0 0, L_0xc10820; 1 drivers
v0xc0fc40_0 .net "d", 0 0, L_0xc10930; 1 drivers
v0xc0fcf0_0 .net "f", 0 0, L_0xc10a10; 1 drivers
v0xc0fd90_0 .net "nots", 0 0, L_0xc10760; 1 drivers
v0xc0fe70_0 .net "s", 0 0, C4<z>; 0 drivers
S_0xbfd230 .scope module, "testbench" "testbench" 3 1;
 .timescale 0 0;
v0xc102f0_0 .var "a", 0 0;
v0xc103c0_0 .var "b", 0 0;
v0xc10470_0 .net "f", 0 0, v0xc101a0_0; 1 drivers
v0xc10520_0 .var "s", 0 0;
S_0xc0ff10 .scope module, "mux_gate" "mux2to1_bh" 3 4, 2 14, S_0xbfd230;
 .timescale 0 0;
v0xc10040_0 .net "a", 0 0, v0xc102f0_0; 1 drivers
v0xc10100_0 .net "b", 0 0, v0xc103c0_0; 1 drivers
v0xc101a0_0 .var "f", 0 0;
v0xc10240_0 .net "s", 0 0, v0xc10520_0; 1 drivers
E_0xc0fcc0 .event edge, v0xc10100_0, v0xc10040_0, v0xc10240_0;
    .scope S_0xc0ff10;
T_0 ;
    %wait E_0xc0fcc0;
    %load/v 8, v0xc10240_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_0.0, 4;
    %load/v 8, v0xc10040_0, 1;
    %set/v v0xc101a0_0, 8, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0xc10100_0, 1;
    %set/v v0xc101a0_0, 8, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0xbfd230;
T_1 ;
    %vpi_call 3 7 "$monitor", " ", $time, " a=%b, b=%b, s=%b f=%b", v0xc102f0_0, v0xc103c0_0, v0xc10520_0, v0xc10470_0;
    %delay 0, 0;
    %set/v v0xc102f0_0, 0, 1;
    %set/v v0xc103c0_0, 1, 1;
    %delay 2, 0;
    %set/v v0xc10520_0, 1, 1;
    %delay 5, 0;
    %set/v v0xc10520_0, 0, 1;
    %delay 10, 0;
    %set/v v0xc102f0_0, 1, 1;
    %set/v v0xc103c0_0, 0, 1;
    %delay 15, 0;
    %set/v v0xc10520_0, 1, 1;
    %delay 20, 0;
    %set/v v0xc10520_0, 0, 1;
    %delay 100, 0;
    %vpi_call 3 14 "$finish";
    %end;
    .thread T_1;
    .scope S_0xbfd230;
T_2 ;
    %vpi_call 3 18 "$dumpfile", "output.vcd";
    %vpi_call 3 19 "$dumpvars";
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "twotoonemux.v";
    "twotoonemux_tb.v";
